#include <iostream>

using namespace std;

int main()
{

	
	int sayi1;
	int sayi2;
	
	cout<<"kucuk sayiyi giriniz:"<<endl;
	cin>>sayi1;
	
	cout<<"buyuk sayiyi giriniz:"<<endl;
	cin>>sayi2;
	
	int adet=0;
	
	
	for (int i=sayi1; i<sayi2 ; i++)
	{
		
      if(i%2==1)
      adet++;
		
		
	}
	cout<<"Tek Sayi:"<<adet<<endl;
	
	
	
	system("pause");
	return 0;
	
}
